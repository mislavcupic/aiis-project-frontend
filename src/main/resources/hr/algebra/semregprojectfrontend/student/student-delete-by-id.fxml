<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox xmlns:fx="http://javafx.com/fxml" fx:controller="hr.algebra.semregprojectfrontend.controller.StudentController" spacing="10" alignment="CENTER">
    <!-- Title for the window -->
    <Label text="Delete Seminar" />

    <!-- Input Field for ID -->
    <Label text="Student ID:" />
    <TextField fx:id="studentIdField" promptText="Enter Student ID" />

    <!-- Button to delete the seminar -->
    <Button text="Delete Student" onAction="#handleDeleteById" />
</VBox>
